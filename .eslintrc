{
  "env":{
    "mocha":true,
    "es6":true,
    "node":true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "rules":{
    "no-underscore-dangle":0,
    "block-scoped-var":2,
    "camelcase":0,
    "comma-style":[
      2,
      "first"
    ],
    "no-bitwise":0,
    "complexity":[
      2,
      11
    ],
    "curly":[
      0,
      "all"
    ],
    "dot-notation":[
      2,
      {
        "allowKeywords":true
      }
    ],
    "eqeqeq":[
      2,
      "allow-null"
    ],
    "strict":[
      2,
      "never"
    ],
    "guard-for-in":2,
    "max-depth":[
      2,
      4
    ],
    "max-statements":[
      2,
      25
    ],
    "new-cap":2,
    "no-caller":2,
    "no-cond-assign":[
      2,
      "except-parens"
    ],
    "no-debugger":2,
    "no-empty":0,
    "no-extend-native":2,
    "no-extra-parens":[
      2,
      "functions"
    ],
    "no-irregular-whitespace":2,
    "no-iterator":2,
    "no-loop-func":2,
    "no-multi-str":2,
    "no-new":2,
    "no-plusplus":2,
    "no-proto":2,
    "no-script-url":2,
    "no-sequences":2,
    "no-shadow":2,
    "no-undef":2,
    "no-unused-vars":[
      2,
      {
        "vars":"local",
        "args":"after-used"
      }
    ],
    "no-use-before-define":0,
    "no-with":2,
    "quotes":[
      2,
      "single"
    ],
    "semi":[
      0,
      "never"
    ],
    "valid-typeof":2,
    "wrap-iife":[
      0,
      "inside"
    ],
    "no-mixed-requires":[
      1,
      true
    ]
  }
}